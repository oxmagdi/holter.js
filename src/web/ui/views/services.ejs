<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <link rel="icon" href="imgs/icon/icon.png">
    <link rel="stylesheet" href="css/uikit.min.css">
    <link rel="stylesheet" href="font-awesome-4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/main.css">
    <title>Holter.js</title>
  </head>
  <body>

    <!-- start navbar -->
    <%- include partials/navbar.ejs %>
    <!-- end navbar -->

   <div class="uk-container">

   <div uk-filter="target: .js-filter">

    <ul class="uk-subnav uk-subnav-pill">
       <li class="uk-active" uk-filter-control><a href="#">All</a></li>
       <li uk-filter-control="[data-running='true']"><a href="#">Active</a></li>
       <li uk-filter-control="[data-running='false']"><a href="#">Not Active</a></li>
    </ul>

    <ul id="services_list" class="js-filter uk-child-width-1-2 uk-child-width-1-3@m uk-text-center" uk-grid>
       <li data-running="true">
           <div class="uk-card uk-card-default uk-card-body uk-text-large" onclick="open_cluster(1)">
                <i class="fa fa-circle fa-xl success" aria-hidden="true"></i> Item
           </div>
       </li>

       <li data-running="false">
           <div class="uk-card uk-card-default uk-card-body uk-text-large"  onclick="open_cluster(1)">
                <i class="fa fa-circle fa-xl danger" aria-hidden="true"></i> Item
           </div>
       </li>

       <li data-running="true">
           <div class="uk-card uk-card-default uk-card-body uk-text-large"  onclick="open_cluster(1)">
                <i class="fa fa-circle fa-xl success" aria-hidden="true"></i> Item
           </div>
       </li>

       <li data-running="false">
           <div class="uk-card uk-card-default uk-card-body uk-text-large"  onclick="open_cluster(1)">
                <i class="fa fa-circle fa-xl danger" aria-hidden="true"></i> Item
           </div>
       </li>

       <li data-running="true">
           <div class="uk-card uk-card-default uk-card-body uk-text-large"  onclick="open_cluster(1)">
                <i class="fa fa-circle fa-xl success" aria-hidden="true"></i> Item
           </div>
       </li>

    </ul>

    </div>

   </div>


   <!-- js -->
   <script type="text/javascript" src="js/uikit.min.js"></script>
   <script type="text/javascript" src="js/uikit-icons.min.js"></script>
   <script type="text/javascript" src="js/services.js"></script>
  </body>
</html>
